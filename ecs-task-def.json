{
  "family": "registration-service",
  "executionRoleArn": "arn:aws:iam::198249843452:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "registration-service-container",
      "image": "198249843452.dkr.ecr.eu-north-1.amazonaws.com/crm-registration-service:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
